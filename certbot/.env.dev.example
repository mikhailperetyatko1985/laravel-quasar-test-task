# Настройки SSL для разработки

# Режим: self-signed (самоподписанный сертификат для разработки)
SSL_MODE=self-signed

# IP адрес (опционально, для CN в сертификате)
SERVER_IP=localhost

# Домен (не используется для self-signed)
SERVER_DOMAIN=

# Email (не требуется для self-signed)
LETSENCRYPT_EMAIL=

# Автоматически создавать сертификат при первом запуске
AUTO_INIT_SSL=yes
